<!DOCTYPE html>
<html>
  <% include _partial/head.ejs %>
  <body>
    <div id="container">
      <div id="wrap">
        <% include _partial/header.ejs %>
        <div class="content">
          <section id="main">
            <article id="post-browser-timing" class="article" itemscope itemprop="blogPost">
              <div class="article-inner">
                <header class="article-header">
                  <h1 class="article-title" itemprop="name"><%- article.title %></h1>
                </header>
                <hr>
                <div class="article-meta">
                  <i class="fa fa-calendar"></i>
                  <time datetime="<%- article.link %>"><%- article.date %></time>
                </div>
                <div class="article-summary">
                  <%- article.summary %>
                </div>
                <hr>
                <div class="article-entry" itemprop="articleBody">
                  <%- article.content %>
                </div>
                <footer class="article-footer">
                  <ul class="article-share-box right">
                    <li class="article-share-btn">
                      <a href="#" class="fa fa-weibo"></a>
                    </li>
                    <li class="article-share-btn">
                      <a href="#" class="fa fa-wechat"></a>
                    </li>
                    <li class="article-share-btn">
                      <a href="#" class="fa fa-youdaoyunbiji"></a>
                    </li>
                  </ul>
                </footer>
              </div>
            </article>
          </section>
          <% include _partial/sidebar.ejs %>
        </div>
        <% include _partial/footer.ejs %>
      </div>
      <% include _partial/mobile-nav.ejs %>
  </body>
</html>